	"version": "2.0.0",
	"tasks": [
		{
			"label": "run qemu",
			"type": "shell",
			"command": "./qemu_aarch64.sh",
			"presentation": {
				"echo": true,
				"clear": true,
				"group": "vm"
			},
			"isBackground": true,
			"problemMatcher": []
		},
		{
			"label": "qemu rootfs update",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/"
			},
			"command": "./rootfs_sync_to_qemu.sh",
			"problemMatcher": [
				"$gcc"
			],
			"isBackground": false,
			"detail": "qemu根目录更新",
		},
		{
			"label": "rootfs update from qemu",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/"
			},
			"command": "./qemu_sync_to_rootfs.sh",
			"problemMatcher": [
				"$gcc"
			],
			"isBackground": false,
			"detail": "外置根目录更新",
		},
